---
import Logo from './Logo.astro'
import Nav from './Nav.astro'

const { current = '' } = Astro.props;
---

<style>
  header {
    display: flex;
    margin: 0 auto;
    max-width: var(--wrap);
    padding: 1em;
    width: 100%;
  }
</style>

<header>
  <Logo width="50" height="50" />
  <Nav current={current} />
</header>
